<div class="responsive-slider-parallax" data-spy="responsive-slider" data-parallax="true" data-parallax-direction="-1" data-transitiontime="1000" data-autoplay="true">
  <div class="slides-container" data-group="slides">
    <ul>
      <li>
        <div class="slide-body" data-group="slide">
          <div class="container">
            <div class="wrapper">
              <div class="caption header" data-animate="slideAppearRightToLeft" data-delay="0" data-length="300">
                <h2>EECS:</h2>
                <p class="sub text-left" data-animate="slideAppearLeftToRight" data-delay="300" data-length="300"><span>Building academic excellence</span></p>
                <p class="sub text-left" data-animate="slideAppearLeftToRight" data-delay="500" data-length="300"><span>through healthy relationships</span></p>
                <p class="sub text-left" data-animate="slideAppearLeftToRight" data-delay="700" data-length="300"><span>and a child-centered approach.</span></p>
              </div>
            </div>
          </div>
        </div>
      </li>
      <li>
        <div class="slide-body" data-group="slide">
          <div class="container">
            <div class="wrapper">
              <div class="caption header text-center" data-animate="slideAppearUpToDown" data-delay="0" data-length="300">
                <h2>The Next Generation of Leaders:</h2>
                <p class="sub" data-animate="slideAppearDownToUp" data-delay="300" data-length="300"><span>Preparing students for the digital age</span></p>
                <p class="sub" data-animate="slideAppearDownToUp" data-delay="500" data-length="300"><span>with a strong focus on math and literacy skills</span></p>
              </div>
            </div>
          </div>
        </div>
      </li>
      <li>
        <div class="slide-body" data-group="slide">
          <div class="container">
            <div class="wrapper">
              <div class="caption header text-right" data-animate="slideAppearDownToUp" data-delay="0" data-length="300">
                <h2>Diversity: <span style="font-weight: 300"><span class="color-alizarin">c</span><span class="color-carrot">o</span><span class="color-sun-flower">l</span><span class="color-emerald">o</span><span class="color-peter-river">r</span></span></h2>
                <p class="sub" data-animate="slideAppearUpToDown" data-delay="300" data-length="300"><span>Building a diverse & vibrant community </span></p>
                <p class="sub" data-animate="slideAppearUpToDown" data-delay="500" data-length="300"><span>through compassionate education.</span></p>
              </div>
            </div>
          </div>
        </div>
      </li>
    </ul>
  </div>
  <a class="slider-control left hidden-xs hidden-sm" href="#" data-jump="prev"><i class="fa fa-angle-left"></i></a>
  <a class="slider-control right hidden-xs hidden-sm" href="#" data-jump="next"><i class="fa fa-angle-right"></i></a>
  <div class="pages-wrapper background-black">
    <ol class="pages">
      <li><a data-jump-to="1">1</a></li>
      <li><a data-jump-to="2">2</a></li>
      <li><a data-jump-to="3">3</a></li>
    </ol>
  </div>
</div>

<!-- Responsive slider - END -->
<script type="text/javascript">
  $(document).ready(function() {
    $('.responsive-calendar').responsiveCalendar({
      time: '2013-05',
      events: {
        "2013-05-30": {"number": 5, "badgeClass": "background-turquoise", "url": "http://w3widgets.com/responsive-slider"},
        "2013-05-26": {"number": 1, "badgeClass": "background-turquoise", "url": "http://w3widgets.com"}, 
        "2013-05-03": {"number": 1, "badgeClass": "background-pomegranate"}, 
        "2013-05-12": {}}
    });

    $('#mixit').mixitup();
  });

  $(window).load(function(){
    if ($(window).width() > 767) {
      matchHeight($('.info-thumbnail .caption .description'), 3);
    }

    $(window).on('resize', function(){
      if ($(window).width() > 767) {
        $('.info-thumbnail .caption .description').height('auto');
        matchHeight($('.info-thumbnail .caption .description'), 3);
      }
    });
  });
</script>

<!-- Events - START -->
<section class="background-white color-black">
  <div class="container">
    <h1 class="text-center x-large">Upcoming Events</h1>
    <div class="news">
      <%= render partial: 'events' %>
    </div>
  </div>
</section>
<!-- Events - FINISH -->

<!-- Info thumbnails - START -->
<section class="">
  <div class="container">
    <h1 class="text-center x-large">Education + Community</h1>
    <p class="lead text-center">At EECS we are focused on helping our students develop the appropriate academic, interpersonal, and creative problem-solving skills they need to thrive in a growing, changing world.</p>
    <div class="row">
      <div class="col-sm-4 col-md-4">
        <div class="info-thumbnail-link">
        <div class="thumbnail info-thumbnail with-icon background-clouds color-text">
          <%= image_tag("stem.png") %>
          <div class="icon">
            <span class="fa-stack">
              <i class="fa fa-circle fa-stack-2x color-lead"></i>
              <i class="fa fa-compass fa-stack-1x color-white"></i>
            </span>
          </div>
          <div class="caption bordered background-white">
            <h3>STEM</h3>
            <p class="description">We are using the enVisionMATH program, designed from the ground up to meet Common Core State Standards using interactive and visual learning techniques. At EECS every student has access to iPads, (talk about the programming class in Rise and Shine here?) film stuff/animation, our science classes... </p>
            <p class="buttons"><a href="#" class="btn btn-lead">Read more</a></p>
          </div>
        </div>
        </div>
      </div>
      <div class="col-sm-4 col-md-4">
        <div class="info-thumbnail-link">
        <div class="thumbnail info-thumbnail with-icon background-clouds color-text">
          <%= image_tag("rise.png") %>
          <div class="icon">
            <span class="fa-stack">
              <i class="fa fa-circle fa-stack-2x color-lead"></i>
              <i class="fa fa-star fa-stack-1x color-white"></i>
            </span>
          </div>
          <div class="caption bordered background-white">
            <h3>Rise & Shine</h3>
            <p class="description">Our Rise and Shine program helps students kick start their day with a lineup of enriching programs. Students take control of their learning by choosing five activities across four separate...connect to their community, leaders, etc </p>
            <p class="buttons"><a href="#" class="btn btn-lead">Read more</a></p>
          </div>
        </div>
        </div>
      </div>
      <div class="col-sm-4 col-md-4">
        <div class="info-thumbnail-link">
        <div class="thumbnail info-thumbnail with-icon background-clouds color-text">
          <%= image_tag("garden1.png") %>
          <div class="icon">
            <span class="fa-stack">
              <i class="fa fa-circle fa-stack-2x color-alizarin"></i>
              <i class="fa fa-eye fa-stack-1x color-white"></i>
            </span>
          </div>
          <div class="caption bordered background-white">
            <h3>Good Citizenship</h3>
            <p class="description">Being good citizens, sustainability, solar power, recycling program, golden bucket, gardens, leadership programs....</p>
            <p class="buttons"><a href="#" class="btn btn-lead">Read more</a></p>
          </div>
        </div>
      </div>
      </div>
    </div>
  </div>
</section>
<!-- Info thumbnails - END -->

